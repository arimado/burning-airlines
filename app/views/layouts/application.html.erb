<!DOCTYPE html>
<html>
<head>
  <title>BurningScaff</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<nav class=navigation>
  <% if @current_user %>
  <%= link_to "All Planes", airplanes_path %>
  <%= link_to "Create Plane", new_airplane_path %>
  <%= link_to "All Flights", flights_path %>
  <%= link_to "Create Flights", new_flight_path %>
  <%= link_to "Search Flights", search_path %>
  <%= link_to "Logout", logout_path, :method => :delete%>
  <% else %>
  <%= link_to "Create an account", users_new_path %>
  <%= link_to "Login", login_path %>
  <% end %>
</nav>


<%= yield %>



</body>
</html>
